<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--返回按钮图标资源-->
    <attr name="title_bar_back_icon" format="reference" />
    <!--标题栏背景色-->
    <attr name="title_bar_background" format="color" />
    <!--标题栏文字颜色-->
    <attr name="title_bar_text_color" format="color" />
    <!--标题栏下方的线是否显示(主要是为了防止标题栏的颜色和内容栏的颜色一样,不能分辨状态栏和内容栏)-->
    <attr name="title_bar_show_line" format="boolean" />
    <!--标题栏下方线的颜色-->
    <attr name="title_bar_line_color" format="color"/>
    <!--loading动画的背景色-->
    <attr name="loading_view_background" format="color"/>
    <!--列表的空页面-->
    <attr name="recycle_view_empty_layout" format="reference"/>
</resources>